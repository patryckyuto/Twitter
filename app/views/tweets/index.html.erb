<div class="container">
  <div class="wrapper col-md-8 col-md-offset-2 col-sm-10">
    <p><%= notice %></p>

    <h1>つぶやき一覧</h1>
      <%= link_to "ツイートを作成する", new_tweet_path, class: 'btn btn-default' %>
    <% @tweets.each do |tweet| %>
    <div class="row">
      <h4><strong><%= tweet.title %></strong></h4>
      <p>ツイート:<%= tweet.content %></p>
      <%= link_to "ツイートを編集する", edit_tweet_path(tweet.id) %>
      <%= link_to "ツイートを削除する", tweet_path(tweet.id), method: :delete, data: { confirm: '本当に削除していいですか？'} %>
    </div>
    <% end %>
  </div>
</div>